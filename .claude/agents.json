{
  "version": 1,
  "agents": {
    "planner": {
      "type": "anthropic",
      "model": "claude-3.6-sonnet",
      "role": "Planner",
      "description": "Task decomposition, roadmap creation, STUDIO plan authoring. No code implementation."
    },
    "architect": {
      "type": "anthropic",
      "model": "claude-3.6-sonnet",
      "role": "Architect",
      "description": "System design authority, reviews STUDIO plans, approves architectural changes."
    },
    "flutter": {
      "type": "external",
      "provider": "antigravity",
      "model": "gemini-3-flash",
      "role": "Flutter Engineer",
      "description": "Frontend Flutter implementation only. No business logic in widgets."
    },
    "backend": {
      "type": "anthropic",
      "model": "claude-3.6-sonnet",
      "role": "Backend Engineer",
      "description": "Supabase, migrations, RLS, repositories."
    },
    "payments": {
      "type": "anthropic",
      "model": "claude-3.6-sonnet",
      "role": "Payments Engineer",
      "description": "Stripe integration, webhooks, signature verification."
    },
    "qa": {
      "type": "anthropic",
      "model": "claude-3.6-sonnet",
      "role": "QA Engineer",
      "description": "Testing, validation, regression checks."
    }
  },
  "routing": {
    "default": "architect",
    "modes": {
      "FLOW": [
        "flutter",
        "backend",
        "qa"
      ],
      "STUDIO": [
        "planner",
        "architect",
        "flutter",
        "backend",
        "payments",
        "qa"
      ]
    }
  }
}